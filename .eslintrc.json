{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard-with-typescript",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "@typescript-eslint/comma-dangle": 0,
    "@typescript-eslint/space-before-function-paren": 0,
    "@typescript-eslint/explicit-function-return-type": 0
  }
}
